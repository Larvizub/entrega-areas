#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/functions-framework/build/src/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/functions-framework/build/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/functions-framework/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/functions-framework/build/src/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/functions-framework/build/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/functions-framework/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules/@google-cloud/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/@google-cloud+functions-framework@3.5.1/node_modules:/mnt/c/Dev/Preact/entrega-areas-gh/functions/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/src/main.js" "$@"
else
  exec node  "$basedir/../../build/src/main.js" "$@"
fi
